PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX xs: <http://www.w3.org/2001/XMLSchema#>

SELECT ?person WHERE {
  ?person foaf:age ?age ;
          foaf:knows ?friend ;
          foaf:name ?name .

  FILTER (?age >= xs:integer(18))
  FILTER (?age < xs:integer(25))
  FILTER (?age != xs:integer(20))
}
